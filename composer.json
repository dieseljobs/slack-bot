{
    "name": "thelhc/lhc-slack-bot",
    "description": "Laravel SlackBot Helper",
    "license": "MIT",
    "authors": [
        {
            "name": "Aaron Kaczmarek",
            "email": "aaronmichaelmusic@gmail.com"
        }
    ],
    "require": {
        "php": "~5.6|~7.0|~7.1|~7.2|~7.3",
        "guzzlehttp/guzzle": "^6.2",
        "illuminate/database": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.*",
        "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.*",
        "thelhc/ip-info": "^1.2"
    },
    "require-dev": {
      "phpunit/phpunit": "^5.4 || ~6.0",
      "orchestra/testbench": "~3.0",
      "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "TheLHC\\SlackBot\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheLHC\\SlackBot\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
